    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "happy-dom": "^20.3.4",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.7",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.0",
    "tailwindcss": "^4",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.17"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "bash -c 'tsc --noEmit'"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
