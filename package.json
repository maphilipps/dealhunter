{
  "name": "lead-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:setup": "./scripts/setup-database.sh",
    "db:setup:migrate": "./scripts/setup-database.sh --migrate-data",
    "worker": "dotenv -e .env.local -e .env -- node -r esbuild-register -r tsconfig-paths/register workers/index.ts",
    "worker:dev": "dotenv -e .env.local -e .env -- nodemon --watch workers --watch lib --ext ts --exec 'node -r esbuild-register -r tsconfig-paths/register workers/index.ts'",
    "worker:pitch": "dotenv -e .env.local -e .env -- node -r esbuild-register -r tsconfig-paths/register workers/pitch.ts",
    "worker:qualifications-scan": "dotenv -e .env.local -e .env -- node -r esbuild-register -r tsconfig-paths/register workers/qualifications-scan.ts",
    "worker:prequal": "dotenv -e .env.local -e .env -- node -r esbuild-register -r tsconfig-paths/register workers/prequal-processing.ts",
    "worker:prequal:dev": "dotenv -e .env.local -e .env -- nodemon --watch workers --watch lib --ext ts --exec 'node -r esbuild-register -r tsconfig-paths/register workers/prequal-processing.ts'",
    "bull-board": "dotenv -e .env.local -e .env -- node -r esbuild-register -r tsconfig-paths/register lib/bullmq/board.ts",
    "db:seed": "npx tsx lib/db/seed.ts",
    "db:seed:test": "npx tsx scripts/seed-test-users.ts",
    "db:seed:dax": "npx tsx scripts/seed-dax-data.ts",
    "db:seed:massive": "npx tsx scripts/seed-massive-data.ts",
    "db:reset": "./scripts/reset-database.sh",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "npm run db:seed:test && playwright test",
    "test:e2e:ui": "npm run db:seed:test && playwright test --ui",
    "test:e2e:debug": "npm run db:seed:test && playwright test --debug",
    "test:all": "npm run test:run && npm run test:e2e",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "prettier --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "eslint --fix",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.12",
