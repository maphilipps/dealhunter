    "sonner": "^2.0.7",
    "streamdown": "^2.0.1",
    "swr": "^2.3.8",
    "tailwind-merge": "^3.3.1",
    "use-stick-to-bottom": "^1.1.1",
    "workflow": "^4.0.1-beta.48",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^10.2.0",
    "@playwright/test": "^1.57.0",
